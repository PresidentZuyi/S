AddButton(ToxicEmotes, {Name="¬• Toxic Laugh Emote",Callback=function()
	local player = game.Players.LocalPlayer;
	local function cleanup()
		if player.PlayerGui:FindFirstChild("PlayEmoteGUI") then
			player.PlayerGui:FindFirstChild("PlayEmoteGUI"):Destroy();
		end
	end
	local function initializeAbility()
		local FlatIdent_8CEDF = 0;
		local character;
		local humanoid;
		local humanoidRootPart;
		local animation;
		local animationTrack;
		local sound;
		local triggerAbility;
		local screenGui;
		local button;
		local dragging;
		local dragInput;
		local dragStart;
		local startPos;
		local update;
		while true do
			if (FlatIdent_8CEDF == 5) then
				button.Text = "Toxic Laugh By Zuyi";
				button.Parent = screenGui;
				dragging = nil;
				dragInput = nil;
				FlatIdent_8CEDF = 6;
			end
			if (FlatIdent_8CEDF == 0) then
				character = player.Character or player.CharacterAdded:Wait();
				humanoid = character:WaitForChild("Humanoid");
				humanoidRootPart = character:WaitForChild("HumanoidRootPart");
				animation = Instance.new("Animation");
				FlatIdent_8CEDF = 1;
			end
			if (FlatIdent_8CEDF == 7) then
				button.InputBegan:Connect(function(input)
					if ((input.UserInputType == Enum.UserInputType.MouseButton1) or (input.UserInputType == Enum.UserInputType.Touch)) then
						local FlatIdent_2D88C = 0;
						while true do
							if (1 == FlatIdent_2D88C) then
								startPos = button.Position;
								input.Changed:Connect(function()
									if (input.UserInputState == Enum.UserInputState.End) then
										dragging = false;
									end
								end);
								break;
							end
							if (FlatIdent_2D88C == 0) then
								dragging = true;
								dragStart = input.Position;
								FlatIdent_2D88C = 1;
							end
						end
					end
				end);
				button.InputChanged:Connect(function(input)
					if ((input.UserInputType == Enum.UserInputType.MouseMovement) or (input.UserInputType == Enum.UserInputType.Touch)) then
						dragInput = input;
					end
				end);
				game:GetService("UserInputService").InputChanged:Connect(function(input)
					if ((input == dragInput) and dragging) then
						update(input);
					end
				end);
				button.MouseButton1Click:Connect(function()
					triggerAbility();
				end);
				break;
			end
			if (FlatIdent_8CEDF == 3) then
				screenGui = Instance.new("ScreenGui");
				screenGui.Name = "PlayEmoteGUI";
				screenGui.Parent = player.PlayerGui;
				button = Instance.new("TextButton");
				FlatIdent_8CEDF = 4;
			end
			if (FlatIdent_8CEDF == 6) then
				dragStart = nil;
				startPos = nil;
				update = nil;
				function update(input)
					local FlatIdent_9622C = 0;
					local delta;
					while true do
						if (FlatIdent_9622C == 0) then
							delta = input.Position - dragStart;
							button.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y);
							break;
						end
					end
				end
				FlatIdent_8CEDF = 7;
			end
			if (FlatIdent_8CEDF == 4) then
				button.Size = UDim2.new(0, 150, 0, 30);
				button.Position = UDim2.new(0.5, -75, 0.5, -15);
				button.BackgroundColor3 = Color3.new(0, 0, 0);
				button.TextColor3 = Color3.new(1, 1, 1);
				FlatIdent_8CEDF = 5;
			end
			if (FlatIdent_8CEDF == 1) then
				animation.AnimationId = "rbxassetid://117834213273979";
				animationTrack = humanoid:LoadAnimation(animation);
				animationTrack.Priority = Enum.AnimationPriority.Action;
				sound = Instance.new("Sound");
				FlatIdent_8CEDF = 2;
			end
			if (FlatIdent_8CEDF == 2) then
				sound.SoundId = "";
				sound.Parent = humanoidRootPart;
				triggerAbility = nil;
				function triggerAbility()
					local FlatIdent_817B0 = 0;
					while true do
						if (FlatIdent_817B0 == 0) then
							sound:Play();
							animationTrack:Play(0.1, 1, 1);
							FlatIdent_817B0 = 1;
						end
						if (FlatIdent_817B0 == 1) then
							task.delay(3, function()
								animationTrack:Stop(0.1);
							end);
							break;
						end
					end
				end
				FlatIdent_8CEDF = 3;
			end
		end
	end
	player.CharacterAdded:Connect(function()
		local FlatIdent_5346B = 0;
		while true do
			if (FlatIdent_5346B == 0) then
				cleanup();
				initializeAbility();
				break;
			end
		end
	end);
	cleanup();
	if player.Character then
		initializeAbility();
	end
end});
